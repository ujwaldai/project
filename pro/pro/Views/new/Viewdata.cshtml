@model List<pro.Models.employee>
@{
    ViewBag.Title = "Viewdata";
    int sn = 0;
}

<h2>Viewdata</h2>
<table border="1">
    <tr>
        <th>SN</th>
        <th>Name</th>
        <th>Address</th>
    </tr>
 @foreach (var data in Model)
            {
<tr>
    <td>@(++sn)</td>
    <td>@(data.name)</td>
    <td>@(data.address)</td>
</tr>
            }
</table>


